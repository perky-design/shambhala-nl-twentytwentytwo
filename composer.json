{
	"name": "perky-design/shambhala-nl-twentytwentytwo",
	"type": "wordpress-theme",
	"version": "0.2.0",
	"description": "Shambhala NL theme (a Twenty Twenty-Two child theme)",
	"keywords": [
		"WordPress",
		"Themes"
	],
	"homepage": "https://github.com/perky-design/shambhala-nl-twentytwentytwo/",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"szepeviktor/phpstan-wordpress": "^1.0.2",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"wp-coding-standards/wpcs": "*",
		"wptrt/wpthemereview": "^0.2.1"
	},
	"scripts": {
		"analyze": "@php ./vendor/bin/phpstan analyze",
		"standards:check": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
		"standards:fix": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf"
	},
	"support": {
		"issues": "https://github.com/perky-design/shambhala-nl-twentytwentytwo/issues",
		"source": "https://github.com/perky-design/shambhala-nl-twentytwentytwo/"
	}
}
